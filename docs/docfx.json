{
  "metadata": [
    {
      "src": [
        {
          "src": "../BetterCollections",
          "exclude": ["**/bin/**", "**/obj/**"],
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Better Collections",
      "_appTitle": "Better Collections",
      "_appFooter": "<span>Copyright Â© <a href=\"https://github.com/libsugar/BetterCollections\">BetterCollections</a>&nbsp;&nbsp;&nbsp;&nbsp;Generated by <strong><a href=\"https://github.com/dotnet/docfx\">DocFX</a></strong></span>",
      "_enableSearch": true,
      "_enableNewTab": true
    }
  }
}